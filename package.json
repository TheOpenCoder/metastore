{
  "name": "metastore-server",
  "version": "1.0.0",
  "description": "",
  "main": "/src/server.ts",
  "scripts": {
    "start": "dotenv -e .env.production.local pnpm build && pnpm serve",
    "dev": "pnpm codegen && dotenv -e .env.development.local nodemon src/server",
    "serve": "node dist/server",
    "build": "tsc",
    "graphql:schema:generate": "ts-node ./src/graphql/schema/generate",
    "graphql:types:generate": "graphql-codegen --config codegen.yml",
    "codegen": "pnpm graphql:schema:generate && pnpm graphql:types:generate",
    "prisma:migrate": "dotenv -e .env.development.local prisma migrate dev --name init",
    "prisma:generate": "prisma generate",
    "prisma:generate:watch": "prisma generate --watch"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@graphql-tools/load-files": "^7.0.0",
    "@graphql-tools/merge": "^9.0.0",
    "@prisma/client": "4.14.0",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "graphql": "^16.6.0",
    "graphql-middleware": "^6.1.34",
    "graphql-scalars": "^1.22.0",
    "graphql-shield": "^7.6.5",
    "graphql-yoga": "^3.9.1",
    "jsonwebtoken": "^9.0.0",
    "lodash": "^4.17.21",
    "serverless-http": "^3.2.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^3.3.1",
    "@graphql-codegen/typescript": "^3.0.4",
    "@graphql-codegen/typescript-resolvers": "^3.2.1",
    "@types/express": "^4.17.17",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/lodash": "^4.14.194",
    "@types/node": "^20.1.3",
    "@typescript-eslint/eslint-plugin": "^5.59.5",
    "@typescript-eslint/parser": "^5.59.5",
    "cross-env": "^7.0.3",
    "dotenv-cli": "^7.2.1",
    "eslint": "^8.40.0",
    "nodemon": "^2.0.22",
    "prettier": "^2.8.8",
    "prisma": "^4.14.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4"
  }
}
